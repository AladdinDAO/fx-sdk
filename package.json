    "test:coverage": "vitest run --coverage"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "decimal.js": "^10.6.0",
    "es-toolkit": "^1.43.0",
    "viem": "^2.43.1"
  }
